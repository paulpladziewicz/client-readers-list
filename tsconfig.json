{
  "compilerOptions": {
    "outDir": "./src/types",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "components": ["src/components/index.ts"],
      "utils": ["src/utils/index.ts"],
      "constants/*": ["src/constants/*"],
      "redux-toolkit/*": ["src/redux-toolkit/*"],
      "images": ["src/images/*"]
    }
  }, 
}
